{
  "name": "@genseki/next",
  "description": "Next.js integration for Genseki",
  "version": "0.1.0-alpha.0",
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc",
    "bundle": "rm -rf dist && tsc -p tsconfig.bundle.json",
    "bundle:watch": "tsc -p tsconfig.bundle.json --watch"
  },
  "dependencies": {
    "@genseki/react": "workspace:^",
    "cookie-es": "^2.0.0",
    "next": "15.2.2",
    "radix3": "^1.1.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "zod": "3.25.53"
  },
  "devDependencies": {
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "tsup": "^8.5.0",
    "type-fest": "^4.41.0",
    "vitest": "^3.0.9"
  }
}
